# HÃ m kiá»ƒm tra IP
check_ip_status() {
    local ip=$1
    echo -e "${YELLOW}ğŸ” Äang kiá»ƒm tra tráº¡ng thÃ¡i IP...${NC}"
    if ping -c 3 "$ip" >/dev/null 2>&1; then
        return 0
    else
        echo -e "${RED}âŒ KhÃ´ng thá»ƒ káº¿t ná»‘i tá»›i IP $ip${NC}"
        return 1
    fi
}
# HÃ m láº¥y thÃ´ng tin IP
get_ip_info() {
    local ip=$1
    echo -e "${CYAN}ğŸ“¡ Äang láº¥y thÃ´ng tin IP...${NC}"
    ip_info=$(curl -s "http://ip-api.com/json/$ip" || echo "{}")
    if [ -z "$ip_info" ] || [ "$ip_info" = "{}" ]; then
        echo -e "${RED}âŒ KhÃ´ng thá»ƒ láº¥y thÃ´ng tin IP${NC}"
        return 1
    fi
    echo "$ip_info"
}
# HÃ m kiá»ƒm tra tá»‘c Ä‘á»™ máº¡ng
check_network_speed() {
    echo -e "${CYAN}âš¡ Äang kiá»ƒm tra tá»‘c Ä‘á»™ máº¡ng...${NC}"
    speed_test=$(curl -s -w "%{speed_download}" -o /dev/null http://speedtest.ftp.otenet.gr/files/test1Mb.db 2>/dev/null || echo "0")
    if [ "$speed_test" = "0" ]; then
        echo "N/A"
        return 1
    fi
    speed_mbps=$(echo "scale=2; $speed_test / 1024 / 1024 * 8" | bc 2>/dev/null || echo "N/A")
    echo "$speed_mbps"
}
# HÃ m kiá»ƒm tra tÃªn Ä‘áº§y Ä‘á»§ (nghiÃªm ngáº·t cho láº§n Ä‘áº§u)
validate_full_name_strict() {
    local name="$1"
    local word_count=$(echo "$name" | wc -w)
    if [ "$word_count" -lt 2 ]; then
        return 1
    fi
    if [[ ! "$name" =~ ^[A-Za-zÃ€-á»¹[:space:]]+$ ]]; then
        return 1
    fi
    case "${name,,}" in
        "test test"|"abc xyz"|"nguyen van a"|"tran thi b"|"le van c"|"admin user"|"user name"|"full name")
            return 1
            ;;
    esac
    return 0
}
# HÃ m kiá»ƒm tra tÃªn cÆ¡ báº£n
validate_full_name_basic() {
    local name="$1"
    if [ -z "$name" ] || [ ${#name} -lt 2 ]; then
        return 1
    fi
    return 0
}
# HÃ m kiá»ƒm tra HTTP proxy
check_http_proxy() {
    local ip=$1
    local port=$2
    local user=$3
    local pass=$4
    echo -e "${CYAN}ğŸ”§ Äang kiá»ƒm tra HTTP proxy...${NC}"
    endpoints=(
        "http://icanhazip.com"
        "http://ifconfig.me/ip"
        "http://checkip.amazonaws.com"
    )
    for endpoint in "${endpoints[@]}"; do
        http_test=$(curl -s -o /dev/null -w "%{http_code}" --proxy "http://$user:$pass@$ip:$port" "$endpoint" --connect-timeout 10 --max-time 15)
        if [ "$http_test" = "200" ]; then
            echo "HTTP Proxy âœ… (tested with $endpoint)"
            return 0
        fi
    done
    echo "HTTP Proxy âŒ (khÃ´ng thá»ƒ káº¿t ná»‘i)"
    return 1
}
